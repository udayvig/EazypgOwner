            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.EazyPG.owner.Activities.HomePageActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.EazyPG.owner.Activities.TenantActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.EazyPG.owner.Activities.StaffActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.EazyPG.owner.Activities.MyPGActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.EazyPG.owner.Activities.ComplaintActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.EazyPG.owner.Activities.SubscriptionActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.EazyPG.owner.Activities.ApplianceActivity"
            android:screenOrientation="portrait" />
        <activity

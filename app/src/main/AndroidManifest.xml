            android:name="com.example.EazyPG.owner.Activities.PaymentActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.EazyPG.owner.Activities.RoomsActivity"
            android:screenOrientation="portrait" />
        <activity android:name="com.example.EazyPG.owner.Activities.BedroomComplaintActivity" />
        <activity android:name="com.example.EazyPG.owner.Activities.FoodComplaintActivity" />
        <activity android:name="com.example.EazyPG.owner.Activities.FacilityComplaintActivity" />
        <activity android:name="com.example.EazyPG.owner.Activities.SecurityComplaintActivity"></activity>
        <activity android:name="com.example.EazyPG.owner.Activities.RoomApplianceDetailsActivity">

        </activity>
    </application>

</manifest>
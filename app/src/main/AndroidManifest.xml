            android:screenOrientation="portrait" />

        <activity android:name="com.example.EazyPG.owner.Activities.TenantDashboardFragmentActivity"/>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />

        <activity
            android:name="com.example.EazyPG.owner.Activities.ApplianceDetailsActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.EazyPG.owner.Activities.PassbookActivity"
